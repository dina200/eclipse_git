package radiatorpackage;

public class SimUnit {
	String botType;
	SimUnit (String type) {
		botType = type;
	}
	 int powerUse() {
		 if ("Тепловая машина".equals(botType)) {
			 System.out.println("Тепловая машина");
			 return 2;
		 } else {
			 System.out.println("Радиатор");
			 return 4;
		 }
	}
}
