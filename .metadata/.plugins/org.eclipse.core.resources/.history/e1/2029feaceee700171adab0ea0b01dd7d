package chap2;

public class Shuffle1 {

	public static void main(String[] args) {
		int x = 3;
		while(x > 0){
		
			if (x > 2){
				System.out.print("a");
				x--;
			} else if (x == 2){
				System.out.print("b c");
				x--;
			} else if (x == 1){
				System.out.print("d");
				x--;				
			}			
			System.out.print("-");
				
		}
		
	}

}
